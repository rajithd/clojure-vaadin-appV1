<web-app>
  <!-- Servlet class taken from first :aot namespace -->
  <servlet>
     <servlet-name>clojure-vaadin</servlet-name>
     <servlet-class>com.example.VaadinServlet</servlet-class>
  </servlet>
  <!-- Servlet is mapped to / by default  -->
  <servlet-mapping>
     <servlet-name>clojure-vaadin</servlet-name>
     <url-pattern>/*</url-pattern>
  </servlet-mapping>
</web-app>
